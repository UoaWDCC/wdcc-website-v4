        "next": "14.2.16",
        "react": "^18",
        "react-dom": "^18",
        "tailwind-merge": "^2.5.4",
        "tailwind-variants": "^0.2.1",
        "three": "^0.170.0"
    },
    "devDependencies": {
        "@flydotio/dockerfile": "^0.5.9",
        "@ianvs/prettier-plugin-sort-imports": "^4.4.0",
        "@types/node": "^22.9.0",
        "@types/react": "^18",
        "@types/react-dom": "^18",
        "eslint": "8.57.1",
        "eslint-config-next": "14.2.16",
        "eslint-config-prettier": "^9.1.0",
