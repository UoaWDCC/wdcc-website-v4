    "private": true,
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "start": "next start",
        "deploy": "fly deploy",
        "deploy:depot": "fly deploy --depot",
        "lint": "next lint",
        "lint:no-cache": "next lint --no-cache",
        "format": "pnpm dlx prettier . --write",
